{
  "name": "wemail/wemail",
  "description": "Send Beautiful Email Newsletters with WordPress",
  "keywords": [
    "wordpress",
    "plugin",
    "email",
    "newsletter"
  ],
  "homepage": "https://getwemail.io",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "weDevs",
      "email": "info@wedevs.com",
      "homepage": "https://wedevs.com"
    },
    {
      "name": "Edi Amin",
      "email": "to.ediamin@gmail.com",
      "homepage": "http://ediamin.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "league/csv": "^7.2",
    "danielstjules/stringy": "~3.1.0",
    "appsero/client": "^1.1"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "dev-master",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "tareq1988/wp-php-cs-fixer": "dev-master",
    "phpcompatibility/phpcompatibility-wp": "dev-master",
    "symfony/var-dumper": "^5.3"
  },
  "scripts": {
    "phpcs": [
      "phpcs -p -s"
    ]
  },
  "autoload": {
    "psr-4": {
      "WeDevs\\WeMail\\": "includes"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
